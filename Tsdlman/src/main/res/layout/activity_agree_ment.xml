<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data class="AgreementBinding">

    </data>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_agree_ment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ts.partner.partnerActivity.AgreeMentActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="@color/zhucecolortitle"
        android:orientation="horizontal"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
        <RelativeLayout
            android:id="@+id/agree_reback"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            style="@style/Widget.AppCompat.Button.Borderless.Colored"
            ></RelativeLayout>
        <ImageView
            android:id="@+id/agree_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/addcard_fanhui"
            android:layout_marginStart="15dp"

            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@id/agree_back"
            android:textColor="@color/yello_701585"
            android:layout_marginStart="7dp"
            android:textSize="18sp"
            android:text="@string/fuwuxieyi"
            />

    </RelativeLayout>
</LinearLayout>

</layout>