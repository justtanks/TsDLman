<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="32dp"
    android:background="@mipmap/setting_linearback"
    >
    <TextView
        android:id="@+id/waitform_dingdan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/item_waitformlist_dingdanhao"
        android:textSize="14sp"
        android:textColor="@color/settingtext"
        android:layout_centerVertical="true"
        android:layout_marginStart="15dp"
        />
    <TextView
        android:id="@+id/waitform_dingdannum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:textColor="@color/settingtext"
        android:text="sssssssss"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/waitform_dingdan"
        android:layout_marginStart="1dp"
        />

</RelativeLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@mipmap/item_form_line"
        />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="126dp"
        android:background="@mipmap/setting_linearback"
        >
        <ImageView
            android:id="@+id/myform_headback"
            android:layout_width="106dp"
            android:layout_height="106dp"
            android:layout_centerVertical="true"
            android:layout_marginStart="15dp"
            android:src="@mipmap/myform_shangbiaoback"
            />
        <ImageView
             android:id="@+id/myform_head"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_centerVertical="true"
            android:layout_marginStart="7dp"
           android:layout_alignStart="@+id/myform_headback"
            />
        <TextView
            android:id="@+id/myform_waitreceive_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/myform_headback"
            android:layout_toRightOf="@id/myform_headback"
            android:layout_marginTop="5dp"
            android:layout_marginStart="7dp"
            android:textSize="14sp"
            android:textColor="@color/textcolor"
            android:text="自助商标注册"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/myform_headback"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="15dp"
            android:layout_marginTop="5dp"
            android:layout_marginStart="7dp"
            android:textSize="14sp"
            android:textColor="@color/myformcolor"
            android:text="@string/waiforrecieveform"
            />
        <TextView
            android:id="@+id/myform_waitreceive_fuwufei"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/myform_headback"
            android:layout_marginStart="7dp"
            android:textSize="12sp"
            android:textColor="@color/textcolor"
            android:layout_below="@id/myform_waitreceive_title"
            android:layout_marginTop="12dp"
            android:text="@string/waitform_receive_fuwufei"
            />
        <TextView
            android:id="@+id/myform_waitreceive_fuwufeinum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/myform_waitreceive_fuwufei"
            android:layout_marginStart="7dp"
            android:textSize="12sp"
            android:textColor="@color/textcolor"
            android:layout_below="@id/myform_waitreceive_title"
            android:layout_marginTop="12dp"
            android:text="￥10000"
            />
        <TextView
            android:id="@+id/myform_waitreceive_guanfeititle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/myform_waitreceive_fuwufeinum"
            android:layout_marginStart="22dp"
            android:textSize="12sp"
            android:textColor="@color/textcolor"
            android:layout_below="@id/myform_waitreceive_title"
            android:layout_marginTop="12dp"
            android:text="@string/guanfei"
            />
        <TextView
            android:id="@+id/myform_waitreceive_guanfei"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/myform_waitreceive_guanfeititle"
            android:layout_marginStart="7dp"
            android:textSize="12sp"
            android:textColor="@color/textcolor"
            android:layout_below="@id/myform_waitreceive_title"
            android:layout_marginTop="12dp"
            android:text="10000￥"
            />
        <TextView
            android:id="@+id/waitform_phonetitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/myform_headback"
            android:layout_marginStart="7dp"
            android:textSize="12sp"
            android:textColor="@color/textcolor"
            android:layout_below="@id/myform_waitreceive_fuwufei"
            android:layout_marginTop="12dp"
            android:text="@string/phone"
            />
        <TextView
            android:id="@+id/waitform_phonenum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/waitform_phonetitle"
            android:layout_marginStart="7dp"
            android:textSize="12sp"
            android:textColor="@color/textcolor"
            android:layout_below="@id/myform_waitreceive_fuwufei"
            android:layout_marginTop="12dp"
            android:text="110"
            />
        <TextView
            android:id="@+id/waitform_allmoney"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/myform_headback"
            android:layout_marginStart="7dp"
            android:textSize="12sp"
            android:textColor="@color/textcolor"
            android:layout_below="@id/waitform_phonetitle"
            android:layout_marginTop="12dp"
            android:text="10000￥"
            />

    </RelativeLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@mipmap/item_form_line"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="85dp"
        android:background="@mipmap/setting_linearback"
        >
        <TextView
            android:id="@+id/waitform_waitmoney"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:textColor="@color/gray1"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="15dp"
            android:layout_marginTop="10dp"
            android:text="1000$"
            />
        <TextView
            android:id="@+id/waitform_heji"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:textColor="@color/gray1"
            android:layout_toLeftOf="@id/waitform_waitmoney"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="10dp"
            android:text="@string/heiji"
            />
        <TextView
            android:id="@+id/waitform_formnum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:textColor="@color/gray1"
            android:layout_toLeftOf="@id/waitform_heji"
            android:layout_marginEnd="50dp"
            android:layout_marginTop="10dp"
            android:text="共两建商品"
            />
        <ImageView
            android:id="@+id/waitform_jiedanbt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/myform_jiedanbt"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="15dp"
            android:layout_marginEnd="15dp"
            />
        <ImageView
            android:id="@+id/waitform_cancelform"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/myform_cancalform"
            android:layout_alignParentBottom="true"
            android:layout_toLeftOf="@+id/waitform_jiedanbt"
            android:layout_marginBottom="15dp"
            android:layout_marginEnd="12dp"
            />
    </RelativeLayout>

</LinearLayout>